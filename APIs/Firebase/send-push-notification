curl --request POST \
  --url https://fcm.googleapis.com/fcm/send \
  --header 'Authorization: key=<server-key>' \
  --header 'Content-Type: application/json' \
  --data '{
        "to":"<token>",
  "notification":
    {
      "title": "<title>",
        "body": "<body>",
        "badge": 1,
        "sound": "Default",
        "image": "<image>"
    }
}'